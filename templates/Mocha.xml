<templateSet group="Mocha">
  <template name="it" value="it('$name$', () =&gt; {&#10;$body$&#10;});" description="Test" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="&quot;test name&quot;" alwaysStopAt="true" />
    <variable name="body" expression="" defaultValue="&quot;'1'.should.equal('2');&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="des" value="describe('$suiteName$', function() {&#10;it('$testName$', function() {&#10;$body$&#10;});&#10;});" description="Test suite boilerplate" toReformat="true" toShortenFQNames="true">
    <variable name="suiteName" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="testName" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="body" expression="" defaultValue="&quot;'1'.should.equal('2');&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>